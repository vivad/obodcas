* { padding:0; margin:0; outline:0; }
body {
	background:#fff url(images/body.gif) repeat-x 0 0;
	font-family: "Trebuchet MS", Arial, sans-serif;
	font-size:12px;
	line-height:16px;
	color:#636363;
}
input, textarea, select { font-family: "Trebuchet MS", Arial, sans-serif; font-size:12px; }

.field { background:#ebebeb; border:solid 1px #dedede; padding:2px;}

a img { border:0; }

a { color:#8b0000; text-decoration: underline; cursor:pointer; }
a:hover { color:#666; text-decoration: none; }

.left, .alignleft { float:left; display:inline; }
.right, .alignright { float:right; display:inline; }

.cl { font-size:0; line-height:0; clear:both; display:block; height:0; }

.al { text-align: left; }
.ar { text-align: right; }
.ac { text-align: center; }

h2 { font-size:14px; line-height:16px; }
h3 { font-size:12px; line-height:14px; text-transform: uppercase; color:#000; }
h4 { font-size:12px; line-height:14px; text-transform: uppercase; color:#000; }


h1#logo { font-size:0; line-height:0; width:200px; height:64px; float:left; }
h1#logo a{ display:block; height:60px; text-indent: -4000px; background:url(images/logo.gif); }

.shell { width:990px; margin:0 auto; padding:6px; background:#fff; }

#header { height:60px; background:url(images/header.gif); position:relative;}

#navigation { float:right; white-space:nowrap; }
#navigation ul{ list-style-type: none; height:60px; font-weight: bold; float:left; font-size:12px;}
#navigation ul li{ float:left; display:inline; }
#navigation ul li a{ float:left; height:60px; line-height:60px; text-decoration: none; color:#fff; padding:0 7px;}
#navigation ul li a.active,
#navigation ul li a:hover{ background:#fff; color:#8b0000; text-decoration: underline;}

#navigation ul li a.special{ float:left; height:60px; line-height:60px; text-decoration: none; color:#fff; padding:0 7px;}
#navigation ul li a.special:hover{ background:#fff; color:#8b00ff; text-decoration: underline;}



#helpline { float:right; width:110px; top:0; right:0; height:51px; background:#8b0000; color:#fff; padding:3px 7px 0 7px; white-space:nowrap; line-height:20px;}
a.helpline-link { color:#fff; background:url(images/helpline-link.gif) no-repeat 0 0; padding:0 0 0 6px; text-decoration: none;}
a.helpline-link:hover { text-decoration: underline;}

#sidebar { float:left; width:200px;}
#content { float:right; width:780px;border:solid 0px #0000ff;}

#main { padding:5px 0 0 0; }

.box { padding:1px; border:solid 1px #dedede; margin-bottom:10px;}
.box h2{ background:#7f7f7f; color:#fff; font-weight: normal; padding:0 5px; position:relative; height:27px; line-height:27px; }
.box h2 span{ position:absolute; width:10px; height:5px; background:url(images/h2-arr.gif); top:27px; right:10px; font-size:0; line-height:0;}
.box-content { padding:5px;}

a.bul { background:url(images/bul.gif) no-repeat 0 center; padding-left:10px;}

.search { }
.search label { display:block; padding-bottom:3px; }

.search .field {margin-bottom:5px; }
.search .inline-field label { display:inline; padding:0; }
.search .inline-field .field { display:inline; margin:0; }
.search input.field { width:175px; }
.search select.field { width:212px; }
.search select.small-field { width:50px; }

.search-submit { width:70px; background:#8b0000; border:0; color:#fff; height:27px; display:block; line-height:26px; cursor:pointer; margin:12px 0 10px 0;}

.categories { min-height:75px; }
.categories ul { list-style-type: none; font-size:13px;}
.categories ul li{ border-bottom:dashed 1px #ccc; padding:5px 0;}
.categories ul li.last{ border-bottom:0; padding-bottom:0;}
.categories ul li a{ color:#5f5f5f; text-decoration: none; background:url(images/cat.gif) no-repeat 0 4px; padding-left:17px;}
.categories ul li a:hover{ color:#8b0000; }

.Area { min-height:75px}
.Area ul { list-style-type: none; font-size:13px;}
.Area ul li{ border-bottom:dashed 1px #ccc; padding:5px 0;}
.Area ul li.last{ border-bottom:0; padding-bottom:0;}
.Area ul li a{ color:#5f5f5f; text-decoration: none; background:url(images/cat.gif) no-repeat 0 4px; padding-left:17px;}
.Area ul li a:hover{ color:#8b0000; }

#slider { height:200px; position:relative; overflow:hidden; }
#slider-holder { width:720px; height:200px; position:relative; overflow:hidden; }
#slider-holder .jcarousel-clip{ width:720px; height:200px; position:relative; overflow:hidden; }
#slider-holder ul{ width:720px; height:200px; position:relative; overflow:hidden; list-style-type: none;}
#slider-holder ul li{ width:720px; height:200px; position:relative; overflow:hidden; float:left; }

#slider-nav { position:absolute; top:175px; left:644px; z-index:2;}
#slider-nav a{ font-size:0; line-height:0; text-indent: -4000px; width:10px; height:10px; border:solid 1px #8b0000; background:#8b0000; float:left; margin-right:5px; }
#slider-nav a:hover,
#slider-nav a.active { background:#fff;}

#other-slider { height:110px; position:relative; overflow:hidden; }
#other-slider -holder { width:720px; height:200px; position:relative; overflow:hidden; }
#other-slider -holder .jcarousel-clip{ width:720px; height:200px; position:relative; overflow:hidden; }
#other-slider -holder ul{ width:720px; height:200px; position:relative; overflow:hidden; list-style-type: none;}
#other-slider -holder ul li{ width:720px; height:200px; position:relative; overflow:hidden; float:left; }

#other-slider -nav { position:absolute; top:175px; left:644px; z-index:2;}
#other-slider -nav a{ font-size:0; line-height:0; text-indent: -4000px; width:10px; height:10px; border:solid 1px #8b0000; background:#8b0000; float:left; margin-right:5px; }
#other-slider -nav a:hover,
#other-slider -nav a.active { background:#fff;}

.index-content {}
.index-content ul{ list-style-type: none;}
.index-content ul li{ position:relative; padding:0px 0px 0px 4px; border:solid 0px #dedede; float:left; width:190px; margin-right:1px; height:65px; overflow:hidden; }
.index-content ul li.last{ margin-right:0; }

.index-content-info{ position:relative; width:770px; top:0px; left:0px;}
.index-content-info h3{ background:#8b0000; color:#fff; padding:6px 5px; }
.index-content-info h4{ font-weight: normal;}
.index-content-info p { font-size:16px; line-height:18px; text-transform: uppercase; font-weight: bold; color:#000; text-align: justify;}
.

.price { display:block; font-size:21px; color:#8b0000; line-height:23px; }

.products {}
.products ul{ list-style-type: none;}
.products ul li{ position:relative; padding:1px; border:solid 1px #dedede; float:left; width:231px; margin-right:9px; height:300px; overflow:hidden; }
.products ul li.last{ margin-right:0; }

.product-info{ position:absolute; width:153px; top:194px; left:0;}
.product-info h3{ background:#8b0000; color:#fff; padding:6px 10px; }
.product-info h4{ font-weight: normal; }
.product-info p{ font-size:16px; line-height:18px; text-transform: uppercase; font-weight: bold; color:#000; padding:5px 0 7px 0;}
.product-info .product-desc{ padding:10px; background:url(images/info.png); width:133px; }

.price { display:block; font-size:21px; color:#8b0000; line-height:23px; }



.more-products { border:solid 1px #dedede; position:relative; height:114px; overflow:hidden; }
.more-products ul{ list-style-type: none; height:94px; position:relative; overflow:hidden; width:805px;}
.more-products ul li{ float:left; width:94px; height:94px; border-right:dashed 1px #ccc; padding:0 10px;}
.more-products ul li.last{ border-right:0; }

.more-products-holder { width:804px; height:94px; position:relative; overflow:hidden; top:10px; left:70px;}
.more-products-holder .jcarousel-clip{ width:804px; height:94px; position:relative; overflow:hidden; }

.more-nav { font-size:0; line-height:0;}
.more-nav a{ position:absolute; top:40px; left:0; width:30px; height:32px; text-indent: -4000px; z-index:3;}
.more-nav a.next{ background:url(images/next.gif); left:910px;}
.more-nav a.prev{ background:url(images/prev.gif); left:20px;}

.cols { padding:15px 0;}
.col { float:left; display:inline; width:217px; margin-right:30px;}
.col-last { margin-right:0;}

.vendors{}
.vendors ul{ list-style-type: none;}
.vendors ul li{ position:relative; padding:1px; border:solid 1px #dedede; float:left; width:230px; margin-right:9px;margin-bottom:3px; height:110px; overflow:hidden; }
.vendors ul li.last{ margin-right:0; }

.vendors-info{ position:absolute; width:225px; top:0px; left:0;}
.vendors-info h3{ background:#8b0000; color:#fff; padding:1px 1px; }
.vendors-info h4{ font-weight: normal;}

.vendors-info p{ font-size:10px; line-height:18px; font-weight: normal; color:#000; padding:1px 0 1px 0;}
.vendors-info .vendors-desc{ padding:5px; background:url(images/info.png); width:220px; }


.vendors-details{}
.vendors-details ul{ list-style-type: none;}
.vendors-details ul li{ position:relative; padding:1px; border:solid 0px #dedede; float:left; width:700px; margin-right:9px;margin-bottom:3px; height:485px; overflow:hidden; }
.vendors-details ul li.last{ margin-right:0; }

.vendors-details-info{ position:absolute; width:650px; top:0px; left:0;}
.vendors-details-info h3{ background:#8b0000; color:#fff; padding:1px 1px;  }
.vendors-details-info h4{ font-weight: bold; font-size:12px}
.vendors-details-info p{ font-size:10px; line-height:18px; font-weight: normal; color:#000; padding:1px 0 1px 0;}
.vendors-details-info .vendors-details-desc{ padding:10px;  width:400px; }
.vendors-details-info .vendors-details-logo{ position:absolute; top:18px; left:475px; padding:1px;  width:220px; height:175px; color:#fff; border:solid 1px #dedede;}
.vendors-details-info .vendors-details-locate{ position:absolute; top:200px; left:345px; padding:1px;  width:350px; height:250px; color:#fff; border:solid 1px #dedede;}
.vendors-details-info .vendors-details-community{ position:absolute; top:18px; left:425px;   width:40px; height:100px; color:#fff;}


h3.ico { background-repeat:no-repeat; background-position:0 2px; padding:6px 0 8px 30px;}
h3.ico1 { background-image:url(images/ico1.gif);}
h3.ico2 { background-image:url(images/ico2.gif);}
h3.ico3 { background-image:url(images/ico3.gif);}
h3.ico4 { background-image:url(images/ico4.gif);}

#footer { height:51px; background:#ebebeb; white-space:nowrap; line-height:50px; padding:0 15px; color:#7b7b7b; margin-top:10px;}
#footer a{ color:#7b7b7b; text-decoration: none;}
#footer a:hover{ color:#000;}
#footer span{ padding:0 2px;}

#firstAnchor {float:left;}
#lastAnchor {float:left;}
#paginationdiv {float:left;}

#previousAnchor {float:left;}
#nextAnchor {float:left;}